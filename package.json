{
  "name": "vue-blaze-slider",
  "version": "0.1.1",
  "description": "Vue 3 integration for Blaze Slider, an high performance slider library.",
  "files": [
    "dist"
  ],
  "main": "dist/vue-blaze-slider.umd.js",
  "module": "dist/vue-blaze-slider.es.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TotomInc/vue-blaze-slider.git"
  },
  "author": "TotomInc <cazade.thomas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TotomInc/vue-blaze-slider/issues"
  },
  "homepage": "https://github.com/TotomInc/vue-blaze-slider#readme",
  "devDependencies": {
    "@babel/types": "^7.19.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.11.7",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.0",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9"
  },
  "dependencies": {
    "blaze-slider": "^1.8.0"
  }
}
